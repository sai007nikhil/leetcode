   "source": [
    "def twoSum(nums: list[int], target: int):\n",
    "    map = {}\n",
    "    for i in range(len(nums)):\n",
    "        diff = target - nums[i]\n",
    "        if diff in map:\n",
    "            return[i, map[diff]]\n",
    "        map[nums[i]] = i"
   ]
  },
